{
    "version": 2,
    "builds": [{ "src": "./app.js", "use": "@vercel/node"}],
    "routes": [{ "src": "/(.*)", "dest": "/app.js" ,      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],

    "headers": {
      "Access-Control-Allow-Origin": "*"
    }
  }]
}
